{
    "name": "@kipprice/toolkip",
    "version": "1.0.0",
    "description": "toolkip library",
    "license": "MIT",
    "author": "Kip Price",
    "test": "jest",
    "main": "src/index.ts",
    "module": "src/index.ts",
    "types": "typings/index",
    "scripts": {
        "build": "npm run build:tsc && npm run build:webpack",
        "build:webpack": "webpack",
        "build:tsc": "tsc -p tsconfig.json --emitDeclarationOnly",
        "watch": "npm run watch:webpack && npm run watch:tsc",
        "watch:webpack": "webpack -w",
        "watch:tsc": "tsc -p tsconfig.json --emitDeclarationOnly -w",
        "test": "npx jest -- -t .test.ts"
    },
    "devDependencies": {
        "@types/jest": "^24.9.0",
        "jest": "^25.1.0",
        "jest-cli": "^24.9.0",
        "terser-webpack-plugin": "^2.3.4",
        "ts-jest": "^24.3.0",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.0-beta",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10"
    }
}
